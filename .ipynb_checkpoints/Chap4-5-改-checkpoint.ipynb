    "# シミュレーションの実行\n",
    "simulator = AerSimulator()\n",
    "compiled_circuit = transpile(qc, simulator)\n",
    "result = simulator.run(compiled_circuit, shots=1024).result()\n",
    "\n",
    "# 結果の取得と可視化\n",
    "answer = result.get_counts()\n",
    "print(answer)\n",
    "plot_histogram(answer)\n",
    "# plt.show()"
   ]
